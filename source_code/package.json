{
  "name": "source_code",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "tsc -p tsconfig.prod.json && next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint '*/**/*.{js,jsx,ts,tsx}'",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.jsx|*.js|*.css|*.json)\"",
    "husky-install": "cd .. && husky install",
    "precommit": "lint-staged",
    "prepare": "cd .. && husky install"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,d.ts}": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "cd source_code && npm run precommit",
      "pre-push": "cd source_code && npm run build"
    }
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@tanstack/react-query": "^5.59.13",
    "@tanstack/react-query-devtools": "^5.59.13",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.452.0",
    "next": "14.2.15",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
